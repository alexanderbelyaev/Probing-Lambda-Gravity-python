phif=-1/(R0*(1+delta));

phi5=Series[phif,{delta,0,4}]//Normal;
phi=phi5/.{delta-> (dr Sin[Omega]+x)/R0};
phit=phi-(phi/.{Omega->0})//ExpandAll//Simplify;
phit=phit-(phit/.{x->0})//ExpandAll;
phit1=phit//Simplify
phit2=phit1/(dr x Sin[Omega]/R0)//ExpandAll//FullSimplify//ExpandAll
phit3=phit2/.{x->0, Sin[Omega]->0}







\
\>", "Input",
